 @article{ DaiZ07,
 Author = {Dai, Zhensheng and Zhu, Jia-Lin},
 Title = {{Dimensional effects on exciton states in nanorings}},
 Journal = {{JOURNAL OF PHYSICS-CONDENSED MATTER}},
 Year = {{2007}},
 Volume = {{19}},
 Number = {{34}},
 Month = {{AUG 29}},
 Abstract = {{Exciton states in nanorings are studied within the framework of the
    effective-mass approximation. The exciton level structure and binding
    energy in one- and two-dimensional nanorings are found to be very
