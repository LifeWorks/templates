@article{ RetABNR06,
Author = {Retzker, A and Aharonov, Y and Botero, A and Nussinov, S and Reznik, B},
Title = {{Aharonov-Bohm effect without closing a loop}},
Journal = {{PHYSICAL REVIEW A}},
Year = {{2006}},
Volume = {{73}},
Number = {{3}},
Month = {{MAR}},
Abstract = {{We discuss the consequences of the Aharonov-Bohm (AB) effect in setups
   involving several charged particles, wherein none of the charged
   particles encloses a closed loop around the magnetic flux. We show that
   in such setups, the AB phase is encoded either in the relative phase of
   a bipartite or multipartite entangled photons states, or alternatively,
   gives rise to an overall AB phase that can be measured relative to
