 @ARTICLE{Lang97,
   author = {N. D. Lang},
   title = {Anomalous Dependence of Resistance on Length in Atomic Wires},
   journal = prl,
   year = {1997},
   volume = {79},
   pages = {1357--1360},
   number = {7},
   abstract = {The resistance of an atomic Na wire between two macroscopic metallic
