pages = {18392-18397},
doi = {10.1073/pnas.0704772104},
year = {2007},
abstract = {We demonstrate theoretically that most of the observed transport properties of graphene sheets at zero magnetic field can be explained by scattering from charged impurities. We find that, contrary to common perception, these properties are not universal but depend on the concentration of charged impurities
               . For dirty samples (250 × 10 cm <
                < 400 × 10 cm), the value of the minimum conductivity at low carrier density is indeed 4
